{
  "rewrites": [
    {
      "source": "/api/:path",
      "destination": "https://resumate-backend.up.railway.app/api/:path"
    },
    {
      "source": "/api/:path/:subpath*",
      "destination": "https://resumate-backend.up.railway.app/api/:path/:subpath*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}